{
	"manifest": "1",
	"version": "1.0.0",
	"elements": [
		{
			"path": "files",
			"name": "Team Cards",
			"version": "1.0.0",
			"settings": {
				"config": {"autopop": true},
				"properties": [
					{
						"type": "group",
						"name": "settings",
						"label": "Settings",
						"properties": [
							{
								"name": "size",
								"label": "Size",
								"type": "select",
								"default": "standard",
								"values": [
									{"name": "Standard", "value": "standard"},
									{"name": "Large", "value": "large"}
								]
							},
							{
								"name": "style",
								"label": "Image Style",
								"type": "select",
								"default": "round",
								"values": [
									{"name": "Circle", "value": "round"},
									{"name": "Fill", "value": "fill"}
								]
							},
							{
								"name": "extra_area",
								"label": "Display extra area",
								"type": "toggle",
								"default": true
							}
						]
					}
				]
			}
		}
	]
}
